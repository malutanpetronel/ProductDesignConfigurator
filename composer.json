{
    "name":         "petro/sylius-product-design-configurator-plugin",
    "license":      "proprietary",
    "type":         "sylius-plugin",
    "description": "Plugin allowing to add WebGl to see a product in 3D and create custom product variant in Sylius.",
    "keywords": ["sylius", "sylius-plugin", "sylius-product-design-configurator-plugin"],
    "homepage":     "http://evozon.com",
    "authors": [
        {
            "name": "Petronel Malutan",
            "email": "petronel.malutan@evozon.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "sylius/sylius": "^1.7.5"
    },
    "require-dev": {
        "vendor/behat/behat": "^3.6.1",
        "vendor/behat/mink-selenium2-driver": "^1.4",
        "vendor/dmore/behat-chrome-extension": "^1.3",
        "vendor/dmore/chrome-mink-driver": "^2.7",
        "vendor/friends-of-behat/mink": "^1.8",
        "vendor/friends-of-behat/mink-browserkit-driver": "^1.4",
        "vendor/friends-of-behat/mink-extension": "^2.4",
        "vendor/friends-of-behat/page-object-extension": "^0.3",
        "vendor/friends-of-behat/suite-settings-extension": "^1.0",
        "vendor/friends-of-behat/symfony-extension": "^2.1",
        "vendor/friends-of-behat/variadic-extension": "^1.3",
        "vendor/lakion/mink-debug-extension": "^1.2.3",
        "vendor/phpspec/phpspec": "^6.1",
        "vendor/phpstan/phpstan": "0.12.25",
        "vendor/phpstan/phpstan-doctrine": "0.12.13",
        "vendor/phpstan/phpstan-webmozart-assert": "0.12.4",
        "vendor/phpunit/phpunit": "^8.5",
        "vendor/sensiolabs/security-checker": "^6.0",
        "vendor/sylius-labs/coding-standard": "^3.1",
        "vendor/symfony/browser-kit": "^4.4",
        "vendor/symfony/debug-bundle": "^4.4|^5.0",
        "vendor/symfony/dotenv": "^4.4|^5.0",
        "vendor/symfony/intl": "^4.4|^5.0",
        "vendor/symfony/web-profiler-bundle": "^4.4|^5.0",
        "symfony/web-server-bundle": "^4.4|^5.0",
        "vimeo/psalm": "3.11.4"
    },
    "autoload": {
        "psr-4": {
            "Petro\\ProductDesignConfiguratorPlugin\\": "src/",
            "Tests\\Petro\\ProductDesignConfiguratorPlugin\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/Application/Kernel.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.7-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-update-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-create-project-cmd": [
            "php bin/create_node_symlink.php"
        ]
    }
}
